# Internal API route will forward requests to this backend endpoint.
# Static Firebase Hosting deployment uses public env vars.
NEXT_PUBLIC_REUNION_API_URL=https://your-backend.example.com/api/v1/chat/

# If backend requires X-API-Key, set it here.
# WARNING: NEXT_PUBLIC_* variables are exposed to browser bundles.
NEXT_PUBLIC_REUNION_API_KEY=replace-with-api-key

# Public contact links shown on the reconciliation report CTA section.
# Use full URLs.
NEXT_PUBLIC_INSTAGRAM_DM_URL=https://www.instagram.com/your_account/
NEXT_PUBLIC_TELEGRAM_DM_URL=https://t.me/your_account
